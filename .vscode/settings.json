{
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"eslint.enable": true,
	"cSpell.words": ["Interceptable", "tsup"],
	"editor.insertSpaces": false,
	"editor.tabSize": 2,
	"json.schemas": [
		{
			"url": "https://cdn.jsdelivr.net/npm/tsup/schema.json",
			"fileMatch": ["package.json", "tsup.config.json"]
		}
	]
}
